<web-app xmlns="http://java.sun.com/xml/ns/j2ee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         version="2.4"
         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/javacard/jcweb-app_3_0.xsd">
    <display-name>Smart Meter POS</display-name>
    <servlet>
        <servlet-name>SmartMeterPOS</servlet-name>
        <servlet-class>edu.colorado.aos.SmartMeterPOS</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SmartMeterPOS</servlet-name>
        <url-pattern>/smartmeterpos</url-pattern>
    </servlet-mapping>
</web-app>
